{
    "version": 2,
    "builds": [
        {
            "src": "./index.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "cron/index.mjs": {
            "runtime": "nodejs18.x"
        }
    },
    "cron": {
        "cron/index.mjs": "0 * * * *"
    }
}